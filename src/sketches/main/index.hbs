{{var "data" htmlWebpackPlugin.options.data}}
<!doctype html>
<html>
  <head>
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
  </head>
  <body>
    <div id="root" class="parent">
      <div class="square-sizer">
        <div class="inner">
          {{#each data.panels}}
            {{> panel}}
          {{/each}}
        </div>
      </div>
    </div>
    <footer>
      <p>
        Click and drag a square.
      </p>
    </footer>
  </body>
</html>